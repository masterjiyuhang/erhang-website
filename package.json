    "crypto-js": "^4.2.0",
    "nuxt": "^3.17.5",
    "nuxt-lodash": "^2.5.3",
    "nuxt-swiper": "^1.2.2",
    "pinia": "^2.3.1",
    "vue": "^3.5.16",
    "vue-router": "^4.5.1"
  },
