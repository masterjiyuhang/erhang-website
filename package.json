    "@nuxt/eslint": "^0.3.13",
    "@nuxt/icon": "^1.13.0",
    "@nuxt/image": "^1.10.0",
    "@nuxtjs/critters": "^0.7.2",
    "@nuxtjs/fontaine": "^0.4.4",
    "@nuxtjs/html-validator": "^1.8.2",
    "@nuxtjs/i18n": "^8.5.6",
